{
    "root": true,
    "parser": "babel-eslint",
    "extends": [
        "eslint:recommended",
        "airbnb"
    ],
    "rules": {
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "jsx-a11y/no-access-key": "off",
        "react/jsx-indent": [
            "error",
            4
        ],
        "react/jsx-indent-props": [
            "error",
            4
        ],
        "react/jsx-one-expression-per-line": 0,
        "quotes": [
            "error",
            "single"
        ],
        "linebreak-style": "off",
        "arrow-parens": [
            "error",
            "as-needed"
        ],
        "arrow-body-style": [
            "error",
            "as-needed"
        ],
        "strict": "off",
        "no-console": "off",
        "import/no-extraneous-dependencies": 0,
        "valid-jsdoc": [2, {
            "prefer": {
                "return": "returns"
            }
        }],
        "no-undef": 1,
        "no-unused-vars": 1
    },
    "plugins": [
        "react",
        "import"
    ],
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jasmine": true,
        "jest": true
    }
}
